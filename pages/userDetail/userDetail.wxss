.swiper-container {
  height: 840rpx;
  margin-bottom: 30rpx;
}
.swiper {
  width: 100%;
  height: 840rpx;
}
.swiper-item {
  width: 100%;
  height: 100%;
}
.photo {
  width: 100%;
  height: 840rpx;
  background-color: #ccc;
  border-radius: 20rpx;
  background-size: cover;
  background-position: center;
  background-repeat: no-repeat;
}
.icon-checked {
  width: 35rpx;
  height: 35rpx;
}
.voice {
  width:200rpx;
  height:60rpx;
}