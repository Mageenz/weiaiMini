.dialog {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.6);
  z-index: 100;
  padding-top: 190rpx;
}
.dialog-content {
  width: 500rpx;
  height: 600rpx;
  background-color: #FFFFFF;
  margin: 0 auto;
  text-align: center;
  border-radius: 14rpx;
  padding-top: 70rpx;
  box-sizing: border-box;
}
.dialog-content image {
  width: 144rpx;
  margin-bottom: 60rpx;
}
.dialog-content view {
  font-size: 24rpx;
  color: #4D4D4D;
  margin-bottom: 20rpx;
}
.btn-register {
  width:450rpx;
  height:90rpx;
  line-height: 90rpx;
  font-size: 30rpx;
  color: #FFFFFF;
  background:rgba(247,98,96,1);
  border-radius:45rpx;
  margin-top: 50rpx;
}
.hover-class {
  opacity: 0.9;
}